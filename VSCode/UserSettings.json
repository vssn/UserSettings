{
  "git.confirmSync": false,
  "window.zoomLevel": 0,
  "files.autoSave": "onFocusChange",
  "editor.fontSize": 15,
  "editor.fontFamily": "'IBM Plex Mono', 'Ubuntu Mono', monospace",
  "editor.lineHeight": 26,
  "workbench.editor.showTabs": true,
  "editor.minimap.renderCharacters": false,
  "editor.fontLigatures": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "python.pythonPath": "/usr/bin/python",
  "python.linting.enabledWithoutWorkspace": false,
  "workbench.activityBar.visible": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "Î²ui - Espresso Soda",
  "alternative.colorTheme": {
    "workbench.colorTheme": "Horizon Italic",
  },
  "gitlens.gitExplorer.files.layout": "auto",
  "gitlens.keymap": "chorded",
  "blade.format.enable": true,
  "csharp.suppressDotnetInstallWarning": true,
  "phpcs.executablePath": "/Users/USERNAME/.composer/vendor/bin/phpcs",
  "workbench.colorCustomizations": {
    "note": "best for light themes",
    "editor.lineHighlightBackground": "#f4fcff",
    "editor.selectionForeground": "#a1ddff",
    "editor.selectionBackground": "#75ccff",
    "terminal.background": "#ffffff",
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "gitlens.views.repositories.files.layout": "auto",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.enabled": false,
  "editor.formatOnSave": true,
  "terminal.integrated.cursorStyle": "line",
  "php.suggest.basic": false
}
